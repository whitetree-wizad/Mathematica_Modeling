@article{智能管道清理机器人,
   title={智能管道清理机器人},
   author={刘进芬;王志娜;赵余婷;刘斌;},
   authoraddress={南京工业大学浦江学院;},
   journal={物联网技术},
   year={2021},
   volume={11},
   number={11},
   pages={96-97+100},
   keywords={智能清理;机器人;共轴反转螺旋式;rpa;管道作业;智能探测},
   abstract={近年来,随着经济的不断增长和科技的不断发展,越来越多的管道埋藏在土地中,用于输送液体、气体以及松散固体。由于管道运输是最实用、最经济的运输方式,所以管道运输在生活和生产中的使用越来越广泛。随之也出现了一些问题亟待解决,尤其是如何清理管道中残留的垃圾以及如何解决因长期使用而导致的管道堵塞问题。智能管道清理机器人由此诞生,它运用了工厂智能云制造系统和螺旋技术解决了上述问题,可以最大程度地节约资源、节省人力、提高效率,逐渐在当今管道运输中发挥着重要作用。},
   isbn/issn={2095-1302},
   notes={61-1483/tp},
   doi={10.16667/j.issn.2095-1302.2021.11.028},
   databaseprovider={cnki},
}

@article{基于升力法的贯流式水轮机叶片设计及可行性分析 ,
   title={基于升力法的贯流式水轮机叶片设计及可行性分析},
   author={朱多彪;李龙;沈云;},
   authoraddress={河海大学能源与电气学院;},
   journal={水电能源科学},
   year={2013},
   volume={31},
   number={07},
   pages={158-161},
   keywords={升力法;贯流式水轮机;叶片;设计;可行性},
   abstract={根据升力法设计轴流式水力机械转轮叶片的基本原理、方法和步骤,设计了贯流式水轮机叶片,建立了贯流式水轮机的三维模型,并基于fluent,采用rngκ-ε双方程湍流模型和simplec算法对设计叶片进行了数值模拟,得到了叶片表面压力和速度云图、流场流线和速度矢量图,并估算出水轮机水力效率。结果表明,采用升力法进行贯流式水轮机叶片设计具有可行性。},
   isbn/issn={1000-7709},
   notes={42-1231/tk},
   databaseprovider={cnki},
}
